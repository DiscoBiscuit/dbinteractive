        <arguments>
          <argument>
            <kind>1</kind>
            <string>var type_event = ds_map_find_value(async_load,"type"); 
switch(type_event) 
{ 
    case network_type_connect: 
        var socket = ds_map_find_value(async_load,"socket");
        ds_list_add(socketlist,socket);
        //send client id
        buffer_seek(Buffer,buffer_seek_start,0);
        buffer_write(Buffer,buffer_u8,1);
        buffer_write(Buffer,buffer_u32,socket);
        network_send_packet(socket,Buffer,buffer_tell(Buffer));
        //tell other clients that someone else joined
        buffer_seek(Buffer,buffer_seek_start,0);
        buffer_write(Buffer,buffer_u8,3);
        buffer_write(Buffer,buffer_u16,socket);
        send_toall();
        //create dummy
        var nn;
        nn = instance_create(0,0,obj_player_dummy);
        nn.clientid = socket;
        break; 
    case network_type_disconnect: 
        var socket = ds_map_find_value(async_load,"socket");
        //tell other clients that someone else left
        buffer_seek(Buffer,buffer_seek_start,0);
        buffer_write(Buffer,buffer_u8,4);
        buffer_write(Buffer,buffer_u16,socket);
        send_toall();
        //remove dummy
        obj_player_dummy.socketcheck = socket;
        with (obj_player_dummy)
        {
            if (socketcheck == clientid)
            {
                instance_destroy();
            }
        }
        var findsocket = ds_list_find_index(socketlist,socket);
        ds_list_delete(socketlist,findsocket);
        break; 
    case network_type_data: 
        var buffer = ds_map_find_value(async_load,"buffer"); 
        var socket = ds_map_find_value(async_load,"id"); 
        buffer_seek(buffer,buffer_seek_start,0); 
        server_receivedpacket(buffer,socket); 
        break; 
} 

</string>
