            <kind>1</kind>
            <string>//Define the spacing between units, since this is in each unit, theoretically you could use custom spacing, but the others wouldnt know and wouldnt move out of the way
spacing = 48

//Reset its destination point, the formation object will take care of that
destx = 0
desty = 0

//If this unit's formation is gone, reset its destination and wait to be commanded again
if !instance_exists(formation)
{
formation = noone
destx = 0
desty = 0
}

//If it has a formation and the formation has somewhere to go...
if formation!=noone
{
if formation.destx!=0 and formation.desty!=0
{
//L is just the length of the formation
l = floor(formation.count/formation.width)
//complicated crap, basically tells each unit to move to a place relative to the formation object based on its number in the formation
destx = formation.x+lengthdir_x((l/2-0.5-floor(formnum/formation.width))*spacing,formation.direction) + lengthdir_x((formation.width/2-0.5-floor(formnum mod formation.width))*spacing,formation.direction+90)
desty = formation.y+lengthdir_y((l/2-0.5-floor(formnum/formation.width))*spacing,formation.direction) + lengthdir_y((formation.width/2-0.5-floor(formnum mod formation.width))*spacing,formation.direction+90)

//if its close to its designated spot in the formation, just make it look like its facing the same direction as everyone else.
if point_distance(x,y,destx,desty)&lt;4
direction = formation.direction
//Handle the unit's direction
image_angle = scr_rotate(image_angle,direction,8)
}
}

//Combat stuff --
//First make sure enemy even exists
if not(instance_exists(target)){
    target=noone
}
//Attack AI unit if close enough
if(target!=noone){
    //Just point and shoot
    if(attackmode==0){
        reloadtimer-=1
    }
    //Break off and follow (shit, here we go) ---
    if(attackmode==1){
        //First, remove itself from the formation
        if(selected==1){
            ds_list_delete(obj_control.selection,selectnum)
        }
        //Now, set the goal to where the target is
        goalx = target.x
        goaly = target.y
        //It should follow the appropriate movement commands in the d&amp;d section
    }
    if(distance_to_object(target)&gt;attackdistance){
        target=noone
    }
}

//Face turrets towards the enemy
if not(target==noone){
    
    for(i=1;i&lt;turretcount;i+=1)
    {
    turrets[i,4] = scr_rotate(turrets[i,4],point_direction(world_x(turrets[i,1],turrets[i,2]),world_y(turrets[i,1],turrets[i,2]),target.x,target.y),8)
    }
} //Face turrets forward
else{
    for(i=1;i&lt;turretcount;i+=1)
    {
    turrets[i,4] = scr_rotate(turrets[i,4],direction,8)
    }
}

//Ensure the unit reloads (and confirm attack)
if(reloadtimer&lt;=0){
    target.unithealth-=10
    reloadtimer=reloadtime
}

//Temporary way for the unit to die
if(unithealth&lt;=0){
    if selected
    ds_list_delete(obj_control.selection,selectnum)
    instance_destroy();
}
</string>
