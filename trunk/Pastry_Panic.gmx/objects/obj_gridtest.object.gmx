        <arguments>
          <argument>
            <kind>1</kind>
            <string>
if(!keyboard_check(vk_shift))
{
    if(mouse_wheel_up()){pitch = min(pitch + 1,90)}
    if(mouse_wheel_down()){pitch = max(pitch - 1,0)}
}
else
{
    if(mouse_wheel_up()){scale += 0.05}
    if(mouse_wheel_down()){scale -= 0.05}
}

if(mouse_check_button(mb_right)){y_offset = mouse_y-room_height/2}

//draw a cross shape so we can clearly see the room center
draw_line(room_width/2,0,room_width/2,room_height)
draw_line(0,room_height/2,room_width,room_height/2)

var sx,sy,tx,ty,dx,dy;

ty = 0

for(j=0;j&lt;grid_y;j+=1)
{
    sy = sin(degtorad(pitch)) * (1+(j/grid_y)*fov*cos(degtorad(pitch)))
    for(i=0;i&lt;grid_x;i+=1)
    {
        sx = 1+(j/grid_y)*cos(degtorad(pitch))
        tx = (i-grid_x/2 + 0.5)*(radius*2)*sx
        
        dx = room_width/2 + tx*scale
        dy = room_height/2 + ty*scale + y_offset
        draw_ellipse(dx - radius*sx*scale,dy - radius*sy*scale,dx + radius*sx*scale, dy +radius*sy*scale,true)
    }
    ty += radius*2*sy
}

draw_text(0,0,"[F]OV: " + string(fov) + "#[R]adius: " + string(radius) + "#[S]cale: " + string(scale) + "#[P]itch: " + string(pitch) + "#Grid [X]: " + string(grid_x) + "#Grid [Y]: " + string(grid_y) + "#Y Offset: " + string(y_offset))
</string>
          </argument>
        </arguments>
      </action>
