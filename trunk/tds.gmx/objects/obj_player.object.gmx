        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement
xmove = false;
ymove = false
if (keyboard_check(ord('W')))
{
    yspeed -= accel;
    ymove = true;
}
if (keyboard_check(ord('A')))
{
    xspeed -= accel;
    xmove = true;
}
if (keyboard_check(ord('S')))
{
    yspeed += accel;
    ymove = true;
}
if (keyboard_check(ord('D')))
{
    xspeed += accel;
    xmove = true;
}
xspeed = clamp(xspeed,-speedmax*1.41,speedmax*1.41);
yspeed = clamp(yspeed,-speedmax*1.41,speedmax*1.41);

var dir,dist,nx,ny;
dir = point_direction(0,0,xspeed,yspeed)
dist = min(speedmax,max(abs(xspeed),abs(yspeed)))
nx = x+lengthdir_x(dist,dir)
ny = y+lengthdir_y(dist,dir)

if place_free(nx,y)
{
    x = nx;
}
else
{
    xspeed = 0;
}
if place_free(x,ny)
{
    y = ny;
}
else
{
    yspeed = 0;
}

if(xmove==false)
{
    xspeed = xspeed*0.9;
}
if(ymove==false)
{
    yspeed = yspeed*0.9;
}

//Rotation
var dif = angle_difference(point_direction(x,y,mouse_x,mouse_y),angle);
var dif2 = angle_difference(point_direction(x,y,mouse_x,mouse_y),angle2);
angle += median(-12,dif,12);
angle2 += median(-17,dif2,17);
angle2 = clamp(angle2,angle-45,angle+45);
</string>
